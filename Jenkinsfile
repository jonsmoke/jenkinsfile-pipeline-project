            echo 'Clarusway_Way to Reinvent Yourself'
              sh 'whoami'
              sh 'pwd'
              sh 'ls'
        }
    }

    stage('run'){
         steps {
            echo 'Clarusway_Way to Reinvent Yourself'
              sh 'whoami'
              sh 'pwd'
              sh 'ls'
        }
    }
}
}